html(lang='en')
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=0.5')
        title #{title}
        link(rel='stylesheet', href='#{baseUrl}/www/css/c.min.css')
        link(rel='icon', type='image/ico', href='#{baseUrl}.favicon.ico')
        script(type='text/javascript', src='#{baseUrl}/www/js/prettify.js')
        if (typeof(googleAnalytics) !== 'undefined')
            include ga
    body(onload='prettyPrint()')
        header
            include ../header
